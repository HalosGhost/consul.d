{
    "datacenter": "Titanus",
    "data_dir": "/opt/consul",
    "log_level": "INFO",
    "node_name": "<fill-in-name>",
    "server": true,
    "ui": true,
    "enable_syslog": true,
    "addresses": {
        "https": "0.0.0.0"
    },
    "ports": {
        "https": 443
    },
    "verify_server_hostname": true,
    "verify_incoming": true,
    "disable_remote_exec": true,
    "key_file": "/etc/ssl/private/<fill-in-name>.pem",
    "cert_file": "/etc/ssl/certs/<fill-in-name>.crt",
    "ca_file": "/etc/ssl/certs/ca-bundle.crt"
}
